---
layout: null
---
[
  {% assign posts = site.pages | where_exp:"p","p.url contains '/posts/'" %}
  {% for page in posts %}
  {
    "title": {{ page.title | jsonify }},
    "url": {{ page.url | relative_url | jsonify }},
    "clean": {{ page.title | replace: "[Việt Hóa]", "" | replace: "[ENG]", "" | split: "-" | first | strip | downcase | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
